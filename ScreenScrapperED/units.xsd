<xs:schema elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:simpleType name="AgeUnits">
        <xs:restriction base="xs:string">
            <xs:enumeration value="days" />
            <xs:enumeration value="months" />
            <xs:enumeration value="years" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="DemographicUnits">

        <xs:restriction base="xs:string">
            <xs:enumeration value="m2"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="DropSensorUnits">

        <xs:restriction base="xs:string">
            <xs:enumeration value="µL"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="SequenceUnits">

        <xs:restriction base="xs:string">
            <xs:enumeration value="mL/h"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="PressureUnits">

        <xs:restriction base="xs:string">
            <xs:enumeration value="mmhg"></xs:enumeration>
            <xs:enumeration value="kPa"></xs:enumeration>
            <xs:enumeration value="PSI"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="WeightUnits">
        <xs:restriction base="xs:string">
            <xs:enumeration value="gram" />
            <xs:enumeration value="kilogram" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="DurationHours">
        <xs:restriction base="xs:string">
            
