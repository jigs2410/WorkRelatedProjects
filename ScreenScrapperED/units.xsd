        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="DurationSecs">
        <xs:restriction base="xs:string">
            <xs:enumeration value="ss" />
           
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="DurationUnits">       
            <xs:union memberTypes="DurationHours  DurationMins DurationSecs" />
    </xs:simpleType>

    <xs:simpleType name="FlowRateUnits">
        <xs:restriction base="xs:string">
            <xs:enumeration value="mL/h" />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="DoseRateUnits">
